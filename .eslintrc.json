{
  "env": {
      "browser": true,
      "es6": true,
      "node": true
  },
  "extends": [
      "airbnb"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "plugins": [

  ],
  "rules": {
      "no-underscore-dangle": "off"
  },
  "overrides": [
      {
          "files": ["*.test.js"],
          "env": {

          },
          "rules": {
            "no-undef": "off"
          }
      }
  ]
}